generator client {
  provider = "prisma-client-js"
  output   = "../generated/mongodb"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URL")
}

model summaryTest {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  totalLocomotive Int
  totalOnDuty Int
  totalOnDepot Int
  totalUnderMaintenance Int
  createdAt DateTime @default(now())
  updatedAt DateTime
}
